{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.2"
    }
  },
  "routes": [
    {
      "src": "/(css|js|build|assets)/(.*)",
      "dest": "/public/$1/$2"
    },
    {
      "src": "/(.*\\.(ico|txt|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}